package ms.leetcode.challenge;

import java.util.Stack;

public class StackEx {

	public static void main(String[] args) {
		// Last In First Out (LIFO)
		Stack<Integer> st =  new Stack<Integer>();
		st.add(10);
		st.add(13);
		st.add(85);
		st.add(4);
		st.add(1);
		
		System.out.println("Stack values "+st);
		
		st.push(45);
		System.out.println("After pushing 45 "+st);
		
		st.pop();
		System.out.println("After poping "+st);
		
		int peek = st.peek();
		System.out.println("After peeking value "+peek);
		
	}

}
