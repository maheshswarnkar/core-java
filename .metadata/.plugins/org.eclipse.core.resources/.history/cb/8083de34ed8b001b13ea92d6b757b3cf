package ms.lambda.ex;

public class Test {
	
	public static int test() {
		int x = 0;
		int y = 0;
		for(int z =0; ; z<5; z++) {
			if((++x >2) || (++y>2) ) {
				x++;
			}
		}
		System.out.println(x+" "+y);
		return 1;
	}
	public static void main(String[] args) {
		int k = test();
		System.out.println("Value of k "+k);
	}

}
